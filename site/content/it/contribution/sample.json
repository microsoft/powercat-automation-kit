{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Sample-Feedback"
    }
  ],
  "pages": [
    {
      "name": "welcome",
      "elements": [
        {
          "type": "radiogroup",
          "name": "group",
          "title": {
            "default": "Which of the following is the best match for you?",
            "it": "Quale delle seguenti Ã¨ la migliore corrispondenza per te?"
          },
          "isRequired": true,
          "colCount": 1,
          "choices": [
            {
              "value": "1",
              "text": {
                "default": "Option 1",
                "it": "Opzione 1"
              }
            },
            {
              "value": "2",
              "text": {
                "default": "Option 2",
                "it": "Opzione 2"
              }
            },
            {
              "value": "3",
              "text": {
                "default": "Option 3",
                "it": "Opzione 3"
              }
            }
          ]
        },
        {
          "type": "checkbox",
          "name": "most-useful",
          "title": {
            "default": "What topics where most useful for you?",
            "it": "Quali argomenti ti sono stati piÃ¹ utili?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": "Not sure",
                "it": "Non sono sicuro"
              }
            },
            {
              "value": "Overview of Authoring",
              "text": {
                "default": "Overview of Authoring",
                "it": "Panoramica della creazione"
              }
            },
            {
              "value": "Authoring goals",
              "text": {
                "default": "Authoring goals",
                "it": "Obiettivi di authoring"
              }
            }
          ]
        },
        {
          "name": "most-useful-topics",
          "type": "ranking",
          "title": {
            "default": "Please rank the topics of interest to you (long tap on selection with a touch screen)",
            "it": "Si prega di classificare gli argomenti di interesse per voi (toccare a lungo la selezione con un touch screen)"
          },
          "visibleIf": "{most-useful.length} > 1",
          "choicesFromQuestion": "most-useful",
          "choicesFromQuestionMode": "selected",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "sample-help",
          "label": {
            "default": "Did this sample help?",
            "it": "Questo esempio ha aiutato?"
          },
          "isRequired": false
        },
        {
          "type": "handoff-task",
          "name": "questionHandoff",
          "title": {
            "default": "Test handoff",
            "it": "Consegna del test"
          },
          "text": {
            "default": "Open",
            "it": "Aperto"
          },
          "destination": "/powercat-automation-kit/contribution/sample?group={group}"
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "it": "Altri commenti?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the office hours based on your responses",
            "it": "Vuoi inviare i risultati del tuo feedback in modo che il team di Core Automation Kit possa personalizzare gli orari di ufficio in base alle tue risposte"
          },
          "label": {
            "default": "Send results?",
            "it": "Inviare risultati?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = false",
          "html": {
            "default": "We hope these questions will help you determine an idea of topics or questions if you attend",
            "it": "Speriamo che queste domande ti aiutino a determinare un'idea di argomenti o domande se partecipi"
          }
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview.",
            "it": "Non vediamo l'ora di ricevere il tuo feedback. Come team principale di Automation Kit utilizziamo il feedback per migliorare la panoramica."
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "it": "Invia il tuo feedback ora?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "it": "Stato: "
          }
        }
      ]
    }
  ]
}