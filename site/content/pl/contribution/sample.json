{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Sample-Feedback"
    }
  ],
  "pages": [
    {
      "name": "welcome",
      "elements": [
        {
          "type": "radiogroup",
          "name": "group",
          "title": {
            "default": "Which of the following is the best match for you?",
            "pl": "Które z poniższych rozwiązań jest dla Ciebie najlepsze?"
          },
          "isRequired": true,
          "colCount": 1,
          "choices": []
        },
        {
          "type": "checkbox",
          "name": "most-useful",
          "title": {
            "default": "What topics where most useful for you?",
            "pl": "Jakie tematy są dla Ciebie najbardziej przydatne?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": "Not sure",
                "pl": "Nie jestem pewien"
              }
            },
            {
              "value": "Overview of Authoring",
              "text": {
                "default": "Overview of Authoring",
                "pl": "Omówienie tworzenia treści"
              }
            },
            {
              "value": "Authoring goals",
              "text": {
                "default": "Authoring goals",
                "pl": "Cele tworzenia"
              }
            }
          ]
        },
        {
          "name": "most-useful-topics",
          "type": "ranking",
          "title": {
            "default": "Please rank the topics of interest to you (long tap on selection with a touch screen)",
            "pl": "Proszę uszeregować interesujące Cię tematy (długie dotknięcie wyboru za pomocą ekranu dotykowego)"
          },
          "visibleIf": "{most-useful.length} > 1",
          "choicesFromQuestion": "most-useful",
          "choicesFromQuestionMode": "selected",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "sample-help",
          "label": {
            "default": "Did this sample help?",
            "pl": "Czy ta próbka pomogła?"
          },
          "isRequired": false
        },
        {
          "type": "handoff-task",
          "name": "questionHandoff",
          "title": {
            "default": "Test handoff",
            "pl": "Przekazanie testu"
          },
          "text": "Open",
          "destination": "/powercat-automation-kit/contribution/sample?group={group}"
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "pl": "Jakieś inne opinie?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the office hours based on your responses",
            "pl": "Czy chcesz przesłać wyniki swojej opinii, aby zespół Core Automation Kit mógł dostosować godziny pracy na podstawie Twoich odpowiedzi?"
          },
          "label": {
            "default": "Send results?",
            "pl": "Wysłać wyniki?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = false",
          "html": {
            "default": "We hope these questions will help you determine an idea of topics or questions if you attend",
            "pl": "Mamy nadzieję, że te pytania pomogą Ci określić pomysł na tematy lub pytania, jeśli weźmiesz udział"
          }
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview.",
            "pl": "Czekamy na Twoją opinię. Jako główny zespół Automation Kit wykorzystujemy informacje zwrotne, aby ulepszyć przegląd."
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "pl": "Wysłać teraz swoją opinię?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "pl": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}