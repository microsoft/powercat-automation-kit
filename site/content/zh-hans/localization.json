{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Localization-Feedback"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "boolean",
          "name": "localization-useful",
          "title": {
            "default": "Is localization of the starter content useful to you?",
            "zh-hans": "初学者内容的本地化对您有用吗？"
          },
          "isRequired": true
        },
        {
          "type": "comment",
          "name": "not-useful",
          "title": {
            "default": "Why was the localized content not useful for you?",
            "zh-hans": "为什么本地化内容对您没有用？"
          },
          "visibleIf": "{localization-useful} = false"
        },
        {
          "type": "checkbox",
          "name": "products",
          "title": {
            "default": "What is most important to you?",
            "zh-hans": "什么对你来说最重要？"
          },
          "isRequired": false,
          "colCount": 1,
          "hasOther": true,
          "choices": [
            {
              "value": "English content is good enough, I do not need a localized version",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "I would rather wait until localized content is on Microsoft Learn platform",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Being able to contribute to improve localizations",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Being able to support ability to load localized content into my organization using the same approach",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Being able to customize the content for my organization and add content that is relevant to our organization",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Localized diagrams",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Using learnings to apply localization to Power BI dashboard",
              "text": {
                "default": null,
                "zh-hans": null
              }
            },
            {
              "value": "Using learnings to apply localization to Power Power Apps",
              "text": {
                "default": null,
                "zh-hans": null
              }
            }
          ]
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "zh-hans": "还有其他反馈吗？"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the localization based on your responses",
            "zh-hans": "是否要发送反馈结果，以便核心自动化工具包团队可以根据您的回复定制本地化"
          },
          "label": {
            "default": "Send results?",
            "zh-hans": "发送结果？"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = false",
          "html": {
            "default": "We hope these questions will help you determine an idea of topics or questions if you attend",
            "zh-hans": "我们希望这些问题能帮助您确定主题或问题的想法，如果您参加"
          }
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview.",
            "zh-hans": "我们期待收到您的反馈。作为核心自动化工具包团队，我们使用反馈来改进概述。"
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "zh-hans": "立即发送您的反馈？"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "zh-hans": "状态：{发送结果状态}"
          }
        }
      ]
    }
  ]
}