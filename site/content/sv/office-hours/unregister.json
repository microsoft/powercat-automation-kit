{
  "checkErrorsMode": "onValueChanging",
  "completedHtml": {
    "default": "Thank you for completing unregistration",
    "sv": "Tack för att du slutförde avregistreringen"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{register-email.length} > 0",
      "setToName": "code",
      "setValue": "Office-Hours-Unregister"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "checkbox",
          "name": "reason",
          "title": {
            "default": "(Optional) Your feedback helps us improve. Which of the following are reason(s) that you are unregistering?",
            "sv": "(Valfritt) Din feedback hjälper oss att förbättra oss. Vilket av följande är anledningar till att du avregistrerar dig?"
          },
          "isRequired": false,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "The time does not work",
              "text": {
                "default": "The time does not work",
                "sv": "Tiden fungerar inte"
              }
            },
            {
              "value": "No longer relevant",
              "text": {
                "default": "No longer relevant",
                "sv": "Inte längre relevant"
              }
            },
            {
              "value": "Too technical",
              "text": {
                "default": "Too technical",
                "sv": "För tekniskt"
              }
            },
            {
              "value": "Not enough business focus",
              "text": {
                "default": "Not enough business focus",
                "sv": "Inte tillräckligt med affärsfokus"
              }
            }
          ]
        },
        {
          "type": "text",
          "name": "register-name",
          "title": {
            "default": "The name that you registered with",
            "sv": "Namnet som du registrerade dig med"
          }
        },
        {
          "type": "text",
          "inputMask": "email",
          "name": "register-email",
          "title": {
            "default": "The email address your registered with",
            "sv": "E-postadressen du registrerade dig med"
          },
          "isRequired": true,
          "validators": [
            {
              "type": "email"
            }
          ]
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your unregistration now?",
            "sv": "Skicka din avregistrering nu?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "sv": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}