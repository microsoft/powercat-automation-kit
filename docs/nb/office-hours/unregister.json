{
  "checkErrorsMode": "onValueChanging",
  "completedHtml": {
    "default": "Thank you for completing unregistration",
    "nb": "Takk for at du fullførte avregistreringen"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{register-email.length} > 0",
      "setToName": "code",
      "setValue": "Office-Hours-Unregister"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "checkbox",
          "name": "reason",
          "title": {
            "default": "(Optional) Your feedback helps us improve. Which of the following are reason(s) that you are unregistering?",
            "nb": "(Valgfritt) Din tilbakemelding hjelper oss med å forbedre oss. Hvilke(n) av følgende er årsak(er) til at du avregistrerer?"
          },
          "isRequired": false,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "The time does not work",
              "text": {
                "default": null,
                "nb": null
              }
            },
            {
              "value": "No longer relevant",
              "text": {
                "default": null,
                "nb": null
              }
            },
            {
              "value": "Too technical",
              "text": {
                "default": null,
                "nb": null
              }
            },
            {
              "value": "Not enough business focus",
              "text": {
                "default": null,
                "nb": null
              }
            }
          ]
        },
        {
          "type": "text",
          "name": "register-name",
          "title": {
            "default": "The name that you registered with",
            "nb": "Navnet du registrerte deg med"
          }
        },
        {
          "type": "text",
          "inputMask": "email",
          "name": "register-email",
          "title": {
            "default": "The email address your registered with",
            "nb": "E-postadressen du registrerte deg med"
          },
          "isRequired": true,
          "validators": [
            {
              "type": "email"
            }
          ]
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your unregistration now?",
            "nb": "Send din avregistrering nå?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "nb": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}