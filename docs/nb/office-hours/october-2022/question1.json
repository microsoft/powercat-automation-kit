{
  "completedHtml": {
    "default": "Thank you for sending feedback",
    "nb": "Takk for at du sender tilbakemelding"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{response.length} > 0",
      "setToName": "code",
      "setValue": "Office-Hours-October-2022-Question-1"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "checkbox",
          "name": "response",
          "title": {
            "default": "Where are you on your Automation Kit Journey?",
            "nb": "Hvor er du på din Automation Kit Journey?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Just started, learning why I should use it",
              "text": {
                "default": "Just started, learning why I should use it",
                "nb": "Nettopp startet, lære hvorfor jeg skal bruke den"
              }
            },
            {
              "value": "Read the starter content and look to install",
              "text": {
                "default": "Read the starter content and look to install",
                "nb": "Les startinnholdet og se etter å installere"
              }
            },
            {
              "value": "Installed in development / demo environment",
              "text": {
                "default": "Installed in development / demo environment",
                "nb": "Installert i utviklings- / demomiljø"
              }
            },
            {
              "value": "Using in production",
              "text": {
                "default": "Using in production",
                "nb": "Bruk i produksjon"
              }
            }
          ]
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "nb": "Send tilbakemelding nå?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "nb": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}