{
  "completedHtml": {
    "default": "Thank you for sending feedback",
    "sv": "Tack för att du skickar feedback"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-result} = true",
      "setToName": "code",
      "setValue": "Office-Hours-October-2022-Feedback"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "rating",
          "name": "nps_score",
          "title": {
            "default": "On a scale of zero to ten, how satisfied with the session today?",
            "sv": "På en skala från noll till tio, hur nöjd med sessionen idag?"
          },
          "isRequired": true,
          "rateMin": 0,
          "rateMax": 10,
          "minRateDescription": "(Most unsatisfied)",
          "maxRateDescription": "(Most satisfied)"
        },
        {
          "type": "checkbox",
          "name": "promoter_features",
          "visibleIf": "{nps_score} >= 9",
          "title": {
            "default": "Which of the following did you value the most?",
            "sv": "Vilket av följande värderade du mest?"
          },
          "isRequired": true,
          "validators": [
            {
              "type": "answercount",
              "text": {
                "default": "Please select no more than three features.",
                "sv": "Välj inte mer än tre funktioner."
              },
              "maxCount": 3
            }
          ],
          "hasOther": true,
          "choices": [
            {
              "value": "Demonstrated Features",
              "text": {
                "default": "Demonstrated Features",
                "sv": "Demonstrerade funktioner"
              }
            },
            {
              "value": "Roadmap of what is next",
              "text": {
                "default": "Roadmap of what is next",
                "sv": "Färdplan för vad som händer härnäst"
              }
            },
            {
              "value": "Question and anwsers",
              "text": {
                "default": "Question and anwsers",
                "sv": "Fråga och anwsers"
              }
            },
            {
              "value": "Learning materials (documentation, demos, code examples)",
              "text": {
                "default": "Learning materials (documentation, demos, code examples)",
                "sv": "Läromedel (dokumentation, demos, kodexempel)"
              }
            }
          ],
          "otherText": "Other features:",
          "colCount": 2
        },
        {
          "type": "comment",
          "name": "passive_experience",
          "visibleIf": "{nps_score} >= 7  and {nps_score} <= 8",
          "title": {
            "default": "What can we do to make your session more satisfying?",
            "sv": "Vad kan vi göra för att göra din session mer tillfredsställande?"
          }
        },
        {
          "type": "comment",
          "name": "disappointing_experience",
          "visibleIf": "{nps_score} <= 6",
          "title": {
            "default": "Please let us know why you had such a disappointing experience with our session",
            "sv": "Låt oss veta varför du hade en så nedslående upplevelse med vår session"
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "sv": "Skicka din feedback nu?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "sv": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}