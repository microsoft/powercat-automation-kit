{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Feedback-June-2023-Release"
    }
  ],
  "pages": [
    {
      "name": "welcome",
      "elements": [
        {
          "type": "checkbox",
          "name": "roles",
          "title": {
            "default": "Which of the following will be of most useful for you in this release?",
            "ja": "このリリースで最も役立つのは次のうちどれですか?"
          },
          "isRequired": true,
          "colCount": 1,
          "hasOther": true,
          "choices": [
            {
              "value": "Scheduler updates",
              "text": {
                "default": "Scheduler updates",
                "ja": "スケジューラの更新"
              }
            },
            {
              "value": "Bug / issue fixes",
              "text": {
                "default": "Bug / issue fixes",
                "ja": "バグ/問題の修正"
              }
            }
          ]
        },
        {
          "type": "comment",
          "name": "anything-else",
          "title": {
            "default": "Any other key features you think should be added?",
            "ja": "追加する必要があると思う他の重要な機能はありますか?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "label": {
            "default": "Do you want to send the feedback on the release?",
            "ja": "リリースに関するフィードバックを送信しますか?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improving our milestones and guidance",
            "ja": "皆様からのフィードバックをお待ちしております。オートメーションキットのコアチームとして、フィードバックを使用してマイルストーンとガイダンスを改善します"
          }
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Did you have any other feedback?",
            "ja": "他にフィードバックはありましたか?"
          },
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "ja": "今すぐフィードバックを送信しますか?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "ja": "地位："
          }
        }
      ]
    }
  ]
}