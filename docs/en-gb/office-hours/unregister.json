{
  "checkErrorsMode": "onValueChanging",
  "completedHtml": {
    "default": "Thank you for completing unregistration",
    "en-gb": "Thank you for completing unregistration"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{register-email.length} > 0",
      "setToName": "code",
      "setValue": "Office-Hours-Unregister"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "checkbox",
          "name": "reason",
          "title": {
            "default": "(Optional) Your feedback helps us improve. Which of the following are reason(s) that you are unregistering?",
            "en-gb": "(Optional) Your feedback helps us improve. Which of the following are reason(s) that you are unregistering?"
          },
          "isRequired": false,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": "Not sure",
                "en-gb": "Not sure"
              }
            },
            {
              "value": "The time does not work",
              "text": {
                "default": "The time does not work",
                "en-gb": "The time does not work"
              }
            },
            {
              "value": "No longer relevant",
              "text": {
                "default": "No longer relevant",
                "en-gb": "No longer relevant"
              }
            },
            {
              "value": "Too technical",
              "text": {
                "default": "Too technical",
                "en-gb": "Too technical"
              }
            },
            {
              "value": "Not enough business focus",
              "text": {
                "default": "Not enough business focus",
                "en-gb": "Not enough business focus"
              }
            }
          ]
        },
        {
          "type": "text",
          "name": "register-name",
          "title": {
            "default": "The name that you registered with",
            "en-gb": "The name that you registered with"
          }
        },
        {
          "type": "text",
          "inputMask": "email",
          "name": "register-email",
          "title": {
            "default": "The email address your registered with",
            "en-gb": "The email address your registered with"
          },
          "isRequired": true,
          "validators": [
            {
              "type": "email"
            }
          ]
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your unregistration now?",
            "en-gb": "Send your unregistration now?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "en-gb": "Status: "
          }
        }
      ]
    }
  ]
}