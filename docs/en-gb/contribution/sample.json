{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Sample-Feedback"
    }
  ],
  "pages": [
    {
      "name": "welcome",
      "elements": [
        {
          "type": "radiogroup",
          "name": "group",
          "title": {
            "default": "Which of the following is the best match for you?",
            "en-gb": "Which of the following is the best match for you?"
          },
          "isRequired": true,
          "colCount": 1,
          "choices": []
        },
        {
          "type": "checkbox",
          "name": "most-useful",
          "title": {
            "default": "What topics where most useful for you?",
            "en-gb": "What topics where most useful for you?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": "Not sure",
                "en-gb": "Not sure"
              }
            },
            {
              "value": "Overview of Authoring",
              "text": {
                "default": "Overview of Authoring",
                "en-gb": "Overview of Authoring"
              }
            },
            {
              "value": "Authoring goals",
              "text": {
                "default": "Authoring goals",
                "en-gb": "Authoring goals"
              }
            }
          ]
        },
        {
          "name": "most-useful-topics",
          "type": "ranking",
          "title": {
            "default": "Please rank the topics of interest to you (long tap on selection with a touch screen)",
            "en-gb": "Please rank the topics of interest to you (long tap on selection with a touch screen)"
          },
          "visibleIf": "{most-useful.length} > 1",
          "choicesFromQuestion": "most-useful",
          "choicesFromQuestionMode": "selected",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "sample-help",
          "label": {
            "default": "Did this sample help?",
            "en-gb": "Did this sample help?"
          },
          "isRequired": false
        },
        {
          "type": "handoff-task",
          "name": "questionHandoff",
          "title": {
            "default": "Test handoff",
            "en-gb": "Test handoff"
          },
          "text": {
            "default": "Open",
            "en-gb": "Open"
          },
          "destination": "/powercat-automation-kit/contribution/sample?group={group}"
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "en-gb": "Any other feedback?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the office hours based on your responses",
            "en-gb": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the office hours based on your responses"
          },
          "label": {
            "default": "Send results?",
            "en-gb": "Send results?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = false",
          "html": {
            "default": "We hope these questions will help you determine an idea of topics or questions if you attend",
            "en-gb": "We hope these questions will help you determine an idea of topics or questions if you attend"
          }
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview.",
            "en-gb": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview."
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "en-gb": "Send your feedback now?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "en-gb": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}