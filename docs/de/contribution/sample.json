{
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Sample-Feedback"
    }
  ],
  "pages": [
    {
      "name": "welcome",
      "elements": [
        {
          "type": "radiogroup",
          "name": "group",
          "title": {
            "default": "Which of the following is the best match for you?",
            "de": "Welche der folgenden Optionen passt am besten zu Ihnen?"
          },
          "isRequired": true,
          "colCount": 1,
          "choices": [
            {
              "value": "1",
              "text": {
                "default": "Option 1",
                "de": "Option 1"
              }
            },
            {
              "value": "2",
              "text": {
                "default": "Option 2",
                "de": "Option 2"
              }
            },
            {
              "value": "3",
              "text": {
                "default": "Option 3",
                "de": "Option 3"
              }
            }
          ]
        },
        {
          "type": "checkbox",
          "name": "most-useful",
          "title": {
            "default": "What topics where most useful for you?",
            "de": "Welche Themen waren für Sie am nützlichsten?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": "Not sure",
                "de": "Weiß nicht"
              }
            },
            {
              "value": "Overview of Authoring",
              "text": {
                "default": "Overview of Authoring",
                "de": "Übersicht über die Dokumenterstellung"
              }
            },
            {
              "value": "Authoring goals",
              "text": {
                "default": "Authoring goals",
                "de": "Erstellungsziele"
              }
            }
          ]
        },
        {
          "name": "most-useful-topics",
          "type": "ranking",
          "title": {
            "default": "Please rank the topics of interest to you (long tap on selection with a touch screen)",
            "de": "Bitte ordnen Sie die Themen, die Sie interessieren (langes Tippen auf die Auswahl mit einem Touchscreen)"
          },
          "visibleIf": "{most-useful.length} > 1",
          "choicesFromQuestion": "most-useful",
          "choicesFromQuestionMode": "selected",
          "isRequired": true
        },
        {
          "type": "boolean",
          "name": "sample-help",
          "label": {
            "default": "Did this sample help?",
            "de": "Hat dieses Beispiel geholfen?"
          },
          "isRequired": false
        },
        {
          "type": "handoff-task",
          "name": "questionHandoff",
          "title": {
            "default": "Test handoff",
            "de": "Testübergabe"
          },
          "text": {
            "default": "Open",
            "de": "Offen"
          },
          "destination": "/powercat-automation-kit/contribution/sample?group={group}"
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "de": "Sonstiges Feedback?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor the office hours based on your responses",
            "de": "Möchten Sie die Ergebnisse Ihres Feedbacks senden, damit das Core Automation Kit-Team die Bürozeiten basierend auf Ihren Antworten anpassen kann?"
          },
          "label": {
            "default": "Send results?",
            "de": "Ergebnisse senden?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = false",
          "html": {
            "default": "We hope these questions will help you determine an idea of topics or questions if you attend",
            "de": "Wir hoffen, dass diese Fragen Ihnen helfen, eine Vorstellung von Themen oder Fragen zu finden, wenn Sie teilnehmen."
          }
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the overview.",
            "de": "Wir freuen uns auf Ihr Feedback. Als Kernteam des Automation Kits nutzen wir das Feedback, um den Überblick zu verbessern."
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "de": "Jetzt Ihr Feedback senden?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "de": "Status: "
          }
        }
      ]
    }
  ]
}