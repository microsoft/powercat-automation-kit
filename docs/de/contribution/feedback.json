{
  "completedHtml": {
    "default": "Thank you for sending feedback",
    "de": "Vielen Dank für Ihr Feedback"
  },
  "triggers": [
    {
      "type": "setvalue",
      "expression": "{send-results} = true",
      "setToName": "code",
      "setValue": "Contribution-Feedback"
    }
  ],
  "pages": [
    {
      "elements": [
        {
          "type": "boolean",
          "name": "content-help",
          "label": {
            "default": "Did this overview help?",
            "de": "Hat diese Übersicht geholfen?"
          },
          "isRequired": false
        },
        {
          "type": "checkbox",
          "name": "most-useful",
          "title": {
            "default": "What topics where most useful for you?",
            "de": "Welche Themen waren für Sie am nützlichsten?"
          },
          "isRequired": true,
          "hasOther": true,
          "colCount": 1,
          "choices": [
            {
              "value": "Not sure",
              "text": {
                "default": null,
                "de": null
              }
            },
            {
              "value": "Link to public feedback channels on GitHub",
              "text": {
                "default": null,
                "de": null
              }
            },
            {
              "value": "Ability to comment publicly on issues or new features",
              "text": {
                "default": null,
                "de": null
              }
            },
            {
              "value": "Knowledge of how can contribute the Automation Kit",
              "text": {
                "default": null,
                "de": null
              }
            }
          ]
        },
        {
          "name": "most-useful-topics",
          "type": "ranking",
          "title": {
            "default": "Please rank the topics of interest to you (long tap on selection with a touch screen)",
            "de": "Bitte ordnen Sie die Themen, die Sie interessieren (langes Tippen auf die Auswahl mit einem Touchscreen)"
          },
          "visibleIf": "{most-useful.length} > 1",
          "choicesFromQuestion": "most-useful",
          "choicesFromQuestionMode": "selected",
          "isRequired": true
        },
        {
          "type": "comment",
          "name": "other-feedback",
          "title": {
            "default": "Any other feedback?",
            "de": "Sonstiges Feedback?"
          }
        },
        {
          "type": "boolean",
          "name": "send-results",
          "title": {
            "default": "Do you want to send the results of the your feedback so that the Core Automation Kit team can tailor our feedback process?",
            "de": "Möchten Sie die Ergebnisse Ihres Feedbacks senden, damit das Core Automation Kit-Team unseren Feedback-Prozess anpassen kann?"
          },
          "label": {
            "default": "Send results?",
            "de": "Ergebnisse senden?"
          },
          "isRequired": true
        },
        {
          "type": "html",
          "visibleIf": "{send-results} = true",
          "html": {
            "default": "We look forward to getting your feedback. As a core Automation Kit team we use the feedback to improve the contribution feedback.",
            "de": "Wir freuen uns auf Ihr Feedback. Als Kernteam des Automation Kits nutzen wir das Feedback, um das Feedback der Beiträge zu verbessern."
          }
        },
        {
          "type": "feedback-task",
          "name": "send-results",
          "title": {
            "default": "Send your feedback now?",
            "de": "Jetzt Ihr Feedback senden?"
          },
          "destination": "https://aka.ms/ak4pp-feedback2",
          "visibleIf": "{send-results} = true"
        },
        {
          "type": "html",
          "visibleIf": "{send-results-status.length} > 0",
          "html": {
            "default": "Status: {send-results-status}",
            "de": "Status: {send-results-status}"
          }
        }
      ]
    }
  ]
}