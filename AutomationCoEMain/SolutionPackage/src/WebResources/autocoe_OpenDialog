// Side Dialog
function OpenSidePanel(){ 
var pageInput = {
    pageType: "custom",
    name: "autocoe_auditlogriskmatrixcustompage_ba6c0",
};
var navigationOptions = {
    target: 2, 
    position: 2,
    width: {value: 600, unit: "px"},
    title: "Risk Matrix"
};
Xrm.Navigation.navigateTo(pageInput, navigationOptions)
    .then(
        function () {
            // Called when the dialog closes
        }
    ).catch(
        function (error) {
            // Handle error
        }
    );
}